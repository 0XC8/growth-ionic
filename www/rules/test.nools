define SkillCal {
    text : '',
    point : 0,
    constructor : function(message, point){
        this.text = message;
        this.point = point;
    }
}

//find any message that starts with hello
rule Server {
    when {
        f1 : SkillCal f1.point == 5 {text: 'dasaf'};
        m  : SkillCal m.text =~ /^server$/;
    }
    then {
        console.log(f1.text);
        modify(m, function(){
          this.text = "后台经验丰富";
        });
    }
}

rule Server {
    when {
        f1 : SkillCal f1.point == 4;
        m  : SkillCal m.text =~ /^server$/;
